{
    "name": "phoenixseo/phoenixseo",
    "description": "phoenixseo.de website",
    "type": "project",
    "homepage": "https://phoenixseo.de",
    "license": "proprietary",
    "authors": [
        {
            "name": "Frank Pfabigan",
            "email": "frank.pfabigan@gmail.com",
            "homepage": "https://phoenixseo.de/",
            "role": "SEO"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1",
        "drupal/admin_toolbar": "^1",
        "drupal/amp": "^3",
        "drupal/amptheme": "^3",
        "drupal/better_exposed_filters": "^3",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8",
        "drupal/ctools": "^3",
        "drupal/eu_cookie_compliance": "^1",
        "drupal/fences": "^2",
        "drupal/field_group": "^3",
        "drupal/file_browser": "^1",
        "drupal/image_effects": "^2",
        "drupal/libraries": "^3",
        "drupal/mailchimp": "^1",
        "drupal/media_entity_browser": "^2",
        "drupal/metatag": "^1",
        "drupal/paragraphs": "^1",
        "drupal/pathauto": "^1",
        "drupal/redirect": "^1",
        "drupal/redirect_404": "^1",
        "drupal/schema_metatag": "^1",
        "drupal/simple_sitemap": "^3",
        "drupal/superfish": "^1",
        "drupal/telephone": "^8",
        "drupal/token": "^1",
        "drupal/video_embed_field": "^2",
        "drush/drush": "^9.0.0",
        "masterminds/html5": "~2.3.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/module_missing_message_fixer": "^1",
        "drupal/imageapi_optimize_resmushit": "^1",
        "drupal/blazy": "^2",
        "drupal/dropzonejs": "^2",
        "drupal/embed": "^1",
        "drupal/entity_embed": "^1",
        "drupal/entity_reference_revisions": "^1",
        "drupal/file_mdm": "^1",
        "drupal/imageapi_optimize": "^2",
        "drupal/inline_entity_form": "^1",
        "drupal/views_bulk_edit": "^2",
        "drupal/views_bulk_operations": "^3",
        "drupal/fontyourface": "^3",
        "drupal/pwa": "^1"
    },
    "conflict": {
        "drupal/drupal": "*",
        "twig/twig": "^1.40.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
